{
    "name": "svelte-remove-attributes",
    "version": "0.0.3",
    "description": "svelte/sveltekit preprocessor to remove attributes like data-testid",
    "main": "./dist/index.js",
    "types": "./dist/index.d.ts",
    "bugs": {
        "url": "https://github.com/jacobbogers/svelte-remove-attributes/issues"
    },
    "repository": {
        "type": "git",
        "url": "https://github.com/jacobbogers/svelte-remove-attributes.git"
    },
    "sideEffects": false,
    "files": [
        "dist",
        "README.md",
        "package.json",
        "LICENSE"
    ],
    "type": "module",
    "scripts": {
        "prebuild": "npm run lint",
        "build": "tsc",
        "prepublishOnly": "npm run build",
        "lint": "biome check --apply src"
    },
    "keywords": [
        "sveltekit",
        "preprocess",
        "preprocessor",
        "svelte",
        "data-testid"
    ],
    "author": "jacob bogers <jkfbogers@gmail.com>",
    "license": "SEE LICENSE IN file LICENSE",
    "engines": {
        "node": ">=v20.14.0"
    },
    "devDependencies": {
        "@biomejs/biome": "1.7.3",
        "@rollup/pluginutils": "5.1.0",
        "@types/node": "18.15.3",
        "svelte": "4.2.18",
        "typescript": "5.0.2"
    },
    "peerDependencies": {
        "@sveltejs/kit": "2 - 2.5",
        "magic-string": ">=0.30.10",
        "svelte": "4 - 5",
        "estree-walker": "3.x"
    }
}